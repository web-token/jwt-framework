        "spomky-labs/aes-key-wrap": "^5.0",
        "web-token/jwt-encryption": "^2.1",
        "web-token/jwt-util-ecc": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "extra": {
        "branch-alias": {
            "v1.0": "1.0.x-dev",
            "v1.1": "1.1.x-dev",
            "v1.2": "1.2.x-dev",
            "v1.3": "1.3.x-dev",
            "v2.0": "2.0.x-dev",
            "v2.1": "2.1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
